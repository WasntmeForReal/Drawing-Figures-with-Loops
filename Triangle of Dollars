using System;

class Program
{
    static void Main()
    {
        int n = int.Parse(Console.ReadLine());
        string dolla = "$ ";
        for (int i = 1; i <= n; i++)
        {
            for (int j = 0; j < i; j++)
            {
                Console.Write(dolla);
            }
            Console.WriteLine();
        }
    }
}
